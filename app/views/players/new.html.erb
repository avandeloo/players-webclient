<h1>Create Player</h1>

<%= form_tag "/players", method: :post do %>
  <div>
    <%= label_tag :first_name %>
    <%= text_field_tag :first_name %>
  </div>
  <div>
    <%= label_tag :last_name %>
    <%= text_field_tag :last_name %>
  </div>
  <div>
    <%= label_tag :position %>
    <%= text_field_tag :position %>
  </div>
  <div>
    <%= label_tag :pro_team %>
    <%= text_field_tag :pro_team %>
  </div>

  <%= submit_tag "Submit" %>
<% end %>